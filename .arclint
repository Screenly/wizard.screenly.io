{
  "linters": {
    "Python linting": {
      "type": "flake8",
      "include": "(\\.py$)",
      "exclude": "(^node_modules/)",
      "severity": {
        "E501": "disabled",
        "E303": "disabled",
        "E265": "disabled"
      }
    },
    "JavaScript linting": {
      "type": "coffeelint",
      "coffeelint.config": ".coffeelint.config",
      "include": "(\\.coffee$)"

    }
  }
}
